{
  "name": "dex-arbitrage",
  "version": "0.0.1",
  "description": "Another flash-loan arbitrage bot",
  "main": "index.ts",
  "directories": {
    "test": "tests",
    "src": "src",
    "dist": "dist"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "createAccount": "ts-node scripts/createAccount.ts",
    "runNode": "npx hardhat node",
    "compile": "npx hardhat compile",
    "deployMainnet": "npx hardhat ignition deploy ignition/modules/aflab_mainnet.ts --network bsc",
    "deployTestnet": "npx hardhat ignition deploy ignition/modules/aflab_testnet.ts --network sepolia",
    "getContractBalance": "ts-node scripts/getContractBalance.ts 0xff00008c249140fA7E02480349C4660Fbb41681e",
    "sendBNBToContract": "ts-node scripts/sendBNBToContract.ts 0xff00008c249140fA7E02480349C4660Fbb41681e 0.01"
  },
