{
  "name": "dex-arbitrage",
  "version": "0.0.1",
  "description": "A DEX flash-loan arbitrage system",
  "main": "dist/index.js",
  "directories": {
    "test": "tests",
    "src": "src",
    "dist": "dist"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "createAccount": "ts-node scripts/createAccount.ts",
    "runNode": "npx hardhat node",
    "deployLocal": "npx hardhat ignition deploy ignition/modules/aflab_testnet.ts --network localhost",
    "deployMainnet": "npx hardhat ignition deploy ignition/modules/aflab_mainnet.ts --network bsc",
    "deployTestnet": "npx hardhat ignition deploy ignition/modules/aflab_testnet.ts --network bsctestnet",
    "printTestnetLogs": "ts-node scripts/printLogs.ts true",
    "printLogs": "ts-node scripts/printLogs.ts false"
  },
  "author": "Jorge Haddad",
  "license": "MIT",
  "dependencies": {
    "@aave/core-v3": "1.19.3",
    "@openzeppelin/contracts": "5.1.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.7",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.12",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@quicknode/sdk": "^2.3.0",
    "@types/node": "^20.2.5",
    "graphviz": "^0.0.9",
    "hardhat": "^2.22.15",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "ethers": "^6.13.4",
    "dotenv": "^16.4.5"
  }
}
